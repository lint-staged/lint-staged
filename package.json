{
  "name": "lint-staged",
  "version": "16.2.3",
  "description": "Lint files staged by git",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lint-staged/lint-staged.git"
  },
  "homepage": "https://github.com/lint-staged/lint-staged#readme",
  "bugs": {
    "url": "https://github.com/lint-staged/lint-staged/issues"
  },
  "author": "Andrey Okonetchnikov <andrey@okonet.ru>",
  "maintainers": [
    "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)"
  ],
  "funding": {
    "url": "https://opencollective.com/lint-staged"
  },
  "engines": {
    "node": ">=20.17"
  },
  "type": "module",
  "bin": {
    "lint-staged": "bin/lint-staged.js"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./bin": "./bin/lint-staged.js",
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "files": [
