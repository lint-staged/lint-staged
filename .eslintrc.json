{
  "extends": [
    "okonet",
    "plugin:node/recommended"
  ],
  "plugins": ["node"],
  "env": {
    "browser": false,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "strict": "off",
    "no-console": "off",
    "node/exports-style": [
      "error",
      "module.exports"
    ]
  }
}
