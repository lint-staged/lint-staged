{
  "extends": [
    "okonet"
  ],
  "env": {
    "browser": false,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "import/no-extraneous-dependencies": [2, {
      "devDependencies": ["**/*.spec.js"]
    }],
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "none"
      }
    ]
  }
}
